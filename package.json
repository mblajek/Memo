{
  "name": "fdds-memo",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm install && vite",
    "build": "pnpm install --prod && vite build --sourcemap",
    "build-debug": "pnpm install --prod && vite build --no-minify --sourcemap=inline",
    "clean": "rm -rf public/build",
    "format": "pnpm install && prettier resources/js --write",
    "typecheck": "pnpm install && tsc --noEmit",
    "typecheck-watch": "pnpm install && tsc --noEmit --watch",
    "test": "pnpm install && vitest run",
    "test-watch": "pnpm install && vitest watch",
    "coverage": "pnpm install && vitest run --coverage",
    "lint": "pnpm install && eslint resources/js",
    "cy:open": "pnpm install && cypress open --e2e"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/luxon": "^3.6.2",
    "@types/wicg-file-system-access": "^2023.10.6",
    "cypress": "^14.4.0",
    "eslint": "^9.28.0",
    "eslint-config-love": "^119.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.19.0",
    "eslint-plugin-promise": "^7.2.1",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "sass": "^1.89.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@eslint/js": "^9.28.0",
    "@felte/core": "^1.4.4",
    "@felte/reporter-solid": "^1.2.11",
    "@felte/solid": "^1.2.14",
    "@felte/validator-zod": "^1.0.18",
    "@floating-ui/core": "^1.7.1",
    "@floating-ui/dom": "^1.7.1",
    "@mbarzda/solid-i18next": "^1.4.1",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.3",
    "@tanstack/query-core": "^5.80.2",
    "@tanstack/solid-query": "^5.80.2",
    "@tanstack/solid-table": "^8.21.3",
    "@tanstack/table-core": "^8.21.3",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.9.0",
    "bowser": "^2.11.0",
    "classnames": "^2.5.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-solid": "^0.14.5",
    "headless-qr": "^1.0.3",
    "html-parse-string": "^0.0.9",
    "i18next": "^25.2.1",
    "i18next-http-backend": "^3.0.2",
    "i18next-intervalplural-postprocessor": "^3.0.0",
    "js-base64": "^3.7.7",
    "laravel-vite-plugin": "^1.3.0",
    "libphonenumber-js": "^1.12.8",
    "luxon": "^3.6.1",
    "rehype-autolink-headings": "^7.1.0",
    "remark-custom-heading-id": "^2.0.0",
    "remark-gfm": "^4.0.1",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.9.7",
    "solid-markdown": "^2.0.14",
    "solid-toast": "^0.5.0",
    "tailwindcss": "^3.4.17",
    "tippy.js": "^6.3.7",
    "typescript-eslint": "^8.33.1",
    "vite": "^6.3.5",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-solid": "^2.11.6",
    "vite-plugin-solid-svg": "^0.8.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.0",
    "zod": "^3.25.49"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "cypress",
      "esbuild"
    ]
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=10",
    "npm": "use-pnpm-instead"
  }
}
