{
  "name": "fdds-memo",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "npm install --include=dev && node git-version.mjs > storage/app/git-version.txt && vite",
    "build": "npm clean-install && vite build && node git-version.mjs > storage/app/git-version.txt",
    "build-debug": "npm clean-install && vite build --no-minify --sourcemap=inline && node git-version.mjs > storage/app/git-version.txt",
    "clean": "rm -rf public/build",
    "format": "prettier resources/js --write",
    "typecheck": "tsc --noEmit",
    "typecheck-watch": "tsc --noEmit --watch",
    "preinstall": "npx -y only-allow npm",
    "test": "vitest run",
    "test-watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "lint": "eslint resources/js"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.6",
    "@testing-library/jest-dom": "^6.4.2",
    "@types/luxon": "^3.4.2",
    "@types/wicg-file-system-access": "^2023.10.4",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-solid": "^0.13.1",
    "jsdom": "^24.0.0",
    "laravel-vite-plugin": "^1.0.1",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-solid": "^2.10.1",
    "vite-plugin-solid-svg": "^0.8.0",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "1.3.1"
  },
  "dependencies": {
    "@felte/core": "^1.4.3",
    "@felte/reporter-solid": "^1.2.10",
    "@felte/solid": "^1.2.13",
    "@felte/validator-zod": "^1.0.17",
    "@mbarzda/solid-i18next": "^1.4.1",
    "@solidjs/meta": "^0.29.3",
    "@solidjs/router": "^0.12.4",
    "@tanstack/solid-query": "^5.24.1",
    "@tanstack/solid-table": "^8.12.0",
    "@zag-js/combobox": "0.26.0",
    "@zag-js/dialog": "0.26.0",
    "@zag-js/hover-card": "^0.26.0",
    "@zag-js/popover": "0.26.0",
    "@zag-js/solid": "0.26.0",
    "axios": "^1.6.7",
    "classnames": "^2.5.1",
    "html-parse-string": "^0.0.9",
    "i18next": "^23.10.0",
    "i18next-http-backend": "^2.5.0",
    "i18next-intervalplural-postprocessor": "^3.0.0",
    "luxon": "^3.4.4",
    "rehype-autolink-headings": "^7.1.0",
    "remark-custom-heading-id": "^1.0.1",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.8.15",
    "solid-markdown": "^2.0.0",
    "solid-toast": "^0.5.0",
    "zod": "^3.22.4"
  }
}
