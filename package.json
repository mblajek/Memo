{
  "name": "fdds-memo",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "npm install --include=dev && node git-version.mjs > storage/app/git-version.txt && vite",
    "build": "npm clean-install && vite build --sourcemap && node git-version.mjs > storage/app/git-version.txt",
    "build-debug": "npm clean-install && vite build --no-minify --sourcemap=inline && node git-version.mjs > storage/app/git-version.txt",
    "clean": "rm -rf public/build",
    "format": "prettier resources/js --write",
    "typecheck": "tsc --noEmit",
    "typecheck-watch": "tsc --noEmit --watch",
    "preinstall": "npx -y only-allow npm",
    "test": "vitest run",
    "test-watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "lint": "eslint resources/js"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.9",
    "@testing-library/jest-dom": "^6.4.8",
    "@types/luxon": "^3.4.2",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-config-love": "^62.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-solid": "^0.14.1",
    "jsdom": "^24.1.1",
    "laravel-vite-plugin": "^1.0.5",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-solid": "^2.10.2",
    "vite-plugin-solid-svg": "^0.8.1",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "2.0.5"
  },
  "dependencies": {
    "@felte/core": "^1.4.3",
    "@felte/reporter-solid": "^1.2.10",
    "@felte/solid": "^1.2.13",
    "@felte/validator-zod": "^1.0.17",
    "@mbarzda/solid-i18next": "^1.4.1",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.14.2",
    "@tanstack/solid-query": "^5.51.21",
    "@tanstack/solid-table": "^8.20.1",
    "@zag-js/combobox": "0.26.0",
    "@zag-js/dialog": "0.26.0",
    "@zag-js/hover-card": "0.26.0",
    "@zag-js/popover": "0.26.0",
    "@zag-js/solid": "0.26.0",
    "axios": "^1.7.4",
    "classnames": "^2.5.1",
    "html-parse-string": "^0.0.9",
    "i18next": "^23.12.3",
    "i18next-http-backend": "^2.5.2",
    "i18next-intervalplural-postprocessor": "^3.0.0",
    "luxon": "^3.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "remark-custom-heading-id": "^2.0.0",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.8.20",
    "solid-markdown": "^2.0.13",
    "solid-toast": "^0.5.0",
    "tippy.js": "^6.3.7",
    "windows-1250": "^3.0.4",
    "zod": "^3.23.8"
  }
}
