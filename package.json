{
  "name": "fdds-memo",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "npm install --include=dev && node git-version.mjs > storage/app/git-version.txt && vite",
    "build": "npm clean-install && vite build --sourcemap && node git-version.mjs > storage/app/git-version.txt",
    "build-debug": "npm clean-install && vite build --no-minify --sourcemap=inline && node git-version.mjs > storage/app/git-version.txt",
    "clean": "rm -rf public/build",
    "format": "prettier resources/js --write",
    "typecheck": "tsc --noEmit",
    "typecheck-watch": "tsc --noEmit --watch",
    "preinstall": "npx -y only-allow npm",
    "test": "vitest run",
    "test-watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "lint": "eslint resources/js",
    "cy:open": "cypress open --e2e"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/luxon": "^3.4.2",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "autoprefixer": "^10.4.20",
    "cypress": "^14.0.0",
    "eslint": "^9.18.0",
    "eslint-config-love": "^117.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-solid": "^0.14.5",
    "jsdom": "^26.0.0",
    "laravel-vite-plugin": "^1.1.1",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "sass": "^1.83.4",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^6.0.9",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-solid": "^2.11.0",
    "vite-plugin-solid-svg": "^0.8.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.2"
  },
  "dependencies": {
    "@felte/core": "^1.4.4",
    "@felte/reporter-solid": "^1.2.11",
    "@felte/solid": "^1.2.14",
    "@felte/validator-zod": "^1.0.18",
    "@floating-ui/dom": "^1.6.13",
    "@mbarzda/solid-i18next": "^1.4.1",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.3",
    "@tanstack/solid-query": "^5.64.2",
    "@tanstack/solid-table": "^8.20.5",
    "axios": "^1.7.9",
    "classnames": "^2.5.1",
    "html-parse-string": "^0.0.9",
    "i18next": "^24.2.1",
    "i18next-http-backend": "^3.0.1",
    "i18next-intervalplural-postprocessor": "^3.0.0",
    "js-base64": "^3.7.7",
    "libphonenumber-js": "^1.11.18",
    "luxon": "^3.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "remark-custom-heading-id": "^2.0.0",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.9.4",
    "solid-markdown": "^2.0.13",
    "solid-toast": "^0.5.0",
    "tippy.js": "^6.3.7",
    "windows-1250": "^3.0.4",
    "zod": "^3.24.1"
  }
}
