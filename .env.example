APP_NAME=Memo
APP_ENV="develop" # local develop testing preprod production
APP_ENV_COLOR=
APP_ENV_FG_COLOR=
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:9081
APP_USER_TIMEZONE="Europe/Warsaw"
APP_BUILD_RELEASE="dev" # pull,dev|prod,release,zip

APP_BACKUP_URL=
APP_BACKUP_AUTH=

TMP_MEETING_NOTIFICATION_SUBJECT="Spotkanie dla {names} bÄ™dzie {datetime}"

DOCS_URL=https://raw.githubusercontent.com/mblajek/Memo/develop/public/docs/

# docker
ME_USER_UID=1000
CLI_USER=me
XDEBUG=0

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=
DB_PORT=
