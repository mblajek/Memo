{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": ["solid"],
    "extends": ["eslint:recommended", "plugin:solid/typescript"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "sort-imports": "off",
        "sort-keys": "off",
        "no-unused-expressions": "warn"
    }
}
